# Frontend Environment Variables Template
# Copy this file to .env.development and fill in your actual values

# Backend API URL
VITE_API_URL=http://localhost:3002

# OLA Maps API Key - Get from https://developer.olamaps.io/
VITE_OLAMAPS_API_KEY=your_olamaps_api_key_here

# Supabase Configuration - Get from https://supabase.com/
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development Settings
VITE_USE_MOCK_OLA_MAPS=true
